{
  "parser": "babel-eslint",
  "extends": "",
  "plugins": [],
  "env": {
    "browser": true,
    "es6": true,
    "phantomjs": true,
    "mocha": true
  },
  "rules": {
    "arrow-parens": [0],
    "no-console": [0],
    "no-bitwise": [0],
    "import/no-extraneous-dependencies": [0],
    "import/extensions": [0],
    "quote-props": [0],
    "class-methods-use-this": [0],
    "max-len": [1, 120, 2]
  }
}
